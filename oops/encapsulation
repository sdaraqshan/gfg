class Car 

{ 

    private String name; 

    public String getName() 

    {  return name;   } 

     

    public void setName(String name) 

    {   this.name=name;  }   } 

 

public class Test 

{ 

       public static void main(String[] args) 

    { 

        Car c=new Car(); 

        c.setName("Honda"); 

        System.out.println(c.getName()); 

    } 

} 
