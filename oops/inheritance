class A 

{ 

    public void print_a()  

    {  System.out.println("printing a");  } 

} 

class B extends A 

{ 

    public void print_b()  

    {   System.out.println("printing b");  } 

} 

public class Test 

{ 

    public static void main(String args[]) 

    { 

    B obj_b=new B(); 

    obj_b.print_a(); 

    obj_b.print_b(); 

} 

}    o/P     

printing a 

printing b 
